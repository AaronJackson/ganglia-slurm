modules {
  module {
    name = "slurm"
    language = "python"
  }
}


collection_group {
  collect_every = 1
  time_threshold = 1

  metric {
    name_match = "slurm_jobs_RUNNING"
    value_threshold = 1.0
  }

  metric {
    name_match = "slurm_jobs_PENDING"
    value_threshold = 1.0
  }

  metric {
    name_match = "slurm_gpu_free"
    value_threshold = 1.0
  }

  metric {
    name_match = "slurm_gpu_alloc"
    value_threshold = 1.0
  }}